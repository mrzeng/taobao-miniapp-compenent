<view class="common_toast_mask">
  <view class="common_toast" >
    <view class="common_toast_title">
      {{title}}
    </view>
    <view class="common_toast_content">
      {{content}}
    </view>
    <view class="common_toast_btns">
      <view class="common_toast_btns_1" onTap="btnFunFirst">{{btnTxt1}}</view>
      <view class="common_toast_btns_2" onTap="btnFunSecond">{{btnTxt2}}</view>
    </view>
  </view>
</view>